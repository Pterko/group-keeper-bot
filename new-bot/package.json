    "grammy-guard": "0.5.0",
    "iso-639-1": "3.1.2",
    "jimp": "^0.22.12",
    "jsdom": "^24.1.1",
    "mongoose": "^8.5.2",
    "node-graceful-shutdown": "^1.1.5",
    "pino": "8.20.0",
    "pino-pretty": "^11.2.2",
    "tsx": "4.7.2",
    "znv": "0.4.0",
    "zod": "3.23.4"

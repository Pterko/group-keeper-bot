    "@grammyjs/hydrate": "1.4.1",
    "@grammyjs/i18n": "1.0.2",
    "@grammyjs/parse-mode": "1.9.0",
    "@grammyjs/runner": "^2.0.3",
    "@grammyjs/types": "3.5.2",
    "@roziscoding/grammy-autoquote": "^2.0.6",
    "@techstark/opencv-js": "^4.9.0-release.3",
    "@types/jsdom": "^21.1.7",
    "async": "^3.2.5",
    "axios": "^1.7.2",
    "callback-data": "^1.1.1",
    "canvas": "^2.11.2",
    "dotenv": "16.4.5",
    "download": "^8.0.0",
    "fastify": "4.26.2",
    "ffmpeg-ffprobe-static": "^6.1.1-rc.5",
